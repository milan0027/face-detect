{
  "swagger": "2.0",
  "info": {
    "title": "API Endpoints",
    "version": "1.0.0"
  },
  "paths": {
    "/": {
      "get": {
        "description": "Returns homepage html",
        "produces": ["text/html"],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/stream": {
      "get": {
        "description": "Returns stream form html",
        "produces": ["text/html"],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      },
      "post": {
        "description": "Takes camera id from request data and redirects to streaming interface",
        "consumes": ["application/x-www-form-urlencoded"],
        "parameters": [
          {
            "name": "Camera id from stream form",
            "in": "body",
            "description": "Form data for camera id",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "room": {
                  "type": "string"
                }
              }
            }
          }
        ]
      }
    },
    "/watch": {
      "get": {
        "description": "Returns watch form html",
        "produces": ["text/html"],
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      },
      "post": {
        "description": "Takes camera id from request data and redirects to watching interface",
        "consumes": ["application/x-www-form-urlencoded"],
        "parameters": [
          {
            "name": "Camera id from watch form",
            "in": "body",
            "description": "Form data for camera id",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "room": {
                  "type": "string"
                }
              }
            }
          }
        ]
      }
    },
    "/stream/{room}": {
        "get": {
            "description": "Returns the streaming interface html for a camera id",
            "produces": [
                "text/html"
            ],
            "parameters": [
                {
                    "name": "room",
                    "in": "path",
                    "description":"camera id",
                    "required": true,
                    "schema":{
                        "type":"string"
                    }
                }
            ],
            "responses": {
                "200": {
                  "description": "Successful operation"
                }
              }
        }
      },
      "/watch/{room}": {
        "get": {
            "description": "Returns the watching interface html for a camera id",
            "produces": [
                "text/html"
            ],
            "parameters": [
                {
                    "name": "room",
                    "in": "path",
                    "description":"camera id",
                    "required": true,
                    "schema":{
                        "type":"string"
                    }
                }
            ],
            "responses": {
                "200": {
                  "description": "Successful operation"
                }
              }
        }
      },
      "/predict": {
        "post": {
          "description": "Get results for an image frame in base64 format",
          "consumes": ["application/json"],
          "produces": ["application/json"],
          "parameters": [
            {
              "name": "base64 image",
              "in": "body",
              "description": "image data encoded in base64 format",
              "required": true,
              "schema": {
                "type": "object",
                "properties": {
                  "image_data_url": {
                    "type": "string"
                  }
                },
                "example": {
                    "image_data_url": "A8jerewEr2...(in base64)"
                  }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Result processed successfully\nresult: base64 encoded output image frame\nmultiple_face: [1] signifies multiple faces, [0,2,3] signifies single face, [2] signifies closed eyes, [3] signifies open eyes\nlive_confidence: value between 0 to 1, value of -1 signifies live confidence was not calculated\ncover_ratio: value between 0 and 1, value of -1 signifies cover ratio was not calculated",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "result": {
                        "type": "string"
                      },
                      "multiple_face": {
                        "type": "string"
                      },
                      "live_confidence": {
                        "type": "string"
                      },
                      "cover_ratio": {
                        "type": "string"
                      }
                    },
                    "example": {
                      "result": "dA34oXd9u3...(in base64)",
                      "multiple_face": "0",
                      "live_confidence": "0.7589",
                      "cover_ratio": "0.1221"
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid request data"
            }
          }
        }
      }
   }
}

